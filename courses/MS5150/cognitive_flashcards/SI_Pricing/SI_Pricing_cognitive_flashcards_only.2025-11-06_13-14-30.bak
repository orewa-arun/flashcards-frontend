      "math_visualizations": {
        "concise": "/* layout=dot */\ndigraph ValuePropositionMatrix {\n    rankdir=LR;\n    node [shape=box, style=filled, margin=0.3, fontsize=11];\n    edge [dir=none];\n\n    subgraph cluster_price_axis {\n        label=\"Price (X-axis)\";\n        P_More [label=\"More\"];\n        P_Same [label=\"The Same\"];\n        P_Less [label=\"Less\"];\n        P_More -> P_Same -> P_Less [style=invis];\n    }\n\n    subgraph cluster_benefit_axis {\n        label=\"Benefits (Y-axis)\";\n        B_More [label=\"More\"];\n        B_Same [label=\"The Same\"];\n        B_Less [label=\"Less\"];\n        B_More -> B_Same -> B_Less [dir=back, style=invis];\n        {rank=same; B_More; B_Same; B_Less;}\n    }\n\n    // Matrix cells and their winning/losing status\n    MM [label=\"More for More\", fillcolor=\"#E6E6FA\"];\n    MS [label=\"More for the Same\", fillcolor=\"#E6E6FA\"];\n    ML [label=\"More for Less\", fillcolor=\"#E6E6FA\"];\n    SM [label=\"The Same for More\", fillcolor=\"#FFDAB9\"];\n    SS [label=\"The Same for the Same\", fillcolor=\"#90EE90\"];\n    SL [label=\"The Same for Less\", fillcolor=\"#E6E6FA\"];\n    LM [label=\"Less for More\", fillcolor=\"#FFDAB9\"];\n    LS [label=\"Less for the Same\", fillcolor=\"#FFDAB9\"];\n    LL [label=\"Less for Much Less\", fillcolor=\"#E6E6FA\"];\n\n    // Visual connection to axes (conceptual)\n    B_More -> {MM, MS, ML} [style=dashed, color=gray];\n    B_Same -> {SM, SS, SL} [style=dashed, color=gray];\n    B_Less -> {LM, LS, LL} [style=dashed, color=gray];\n\n    P_More -> {MM, SM, LM} [style=dashed, color=gray];\n    P_Same -> {MS, SS, LS} [style=dashed, color=gray];\n    P_Less -> {ML, SL, LL} [style=dashed, color=gray];\n\n    // Winning propositions (Top row and Rightmost column)\n    subgraph cluster_winning {\n        label=\"Winning Propositions\";\n        color=green;\n        W1 [label=\"W1: More for More\n(High Price, High Benefit)\", fillcolor=\"#C8E6C9\"];\n        W2 [label=\"W2: More for the Same\n(Same Price, High Benefit)\", fillcolor=\"#C8E6C9\"];\n        W3 [label=\"W3: More for Less\n(Low Price, High Benefit)\", fillcolor=\"#C8E6C9\"];\n        W4 [label=\"W4: The Same for Less\n(Low Price, Same Benefit)\", fillcolor=\"#C8E6C9\"];\n        W5 [label=\"W5: Less for Much Less\n(Very Low Price, Low Benefit)\", fillcolor=\"#C8E6C9\"];\n        W1 -> W2 -> W3 [color=green, penwidth=2];\n        ML -> W3 [style=invis];\n        W3 -> W4 -> W5 [color=green, penwidth=2];\n    }\n\n    // Losing propositions (Bottom row and Leftmost column)\n    subgraph cluster_losing {\n        label=\"Losing Propositions\";\n        color=red;\n        L1 [label=\"L1: Less for More\n(High Price, Low Benefit)\", fillcolor=\"#FFCDD2\"];\n        L2 [label=\"L2: Less for the Same\n(Same Price, Low Benefit)\", fillcolor=\"#FFCDD2\"];\n        L3 [label=\"L3: The Same for More\n(High Price, Same Benefit)\", fillcolor=\"#FFCDD2\"];\n        L1 -> L2 [color=red, penwidth=2];\n        L3 -> L1 [color=red, penwidth=2];\n    }\n\n    {rank=same; P_More; B_More; MM; MS; ML; W1; W2; W3;}\n    {rank=same; P_Same; B_Same; SM; SS; SL; W4; L3;}\n    {rank=same; P_Less; B_Less; LM; LS; LL; W5; L1; L2;}\n\n    MM -> W1 [style=invis]; MS -> W2 [style=invis]; ML -> W3 [style=invis];\n    SL -> W4 [style=invis]; LL -> W5 [style=invis];\n    LM -> L1 [style=invis]; LS -> L2 [style=invis]; SM -> L3 [style=invis];\n}",
        "analogy": "/* layout=dot */\ndigraph AnalogyMath {\n    node [shape=box, style=filled, margin=0.3, fontsize=11];\n    \n    Compass [label=\"Strategic Compass\n(Value Matrix)\", fillcolor=\"#ADD8E6\"];\n    Coordinates [label=\"Input: (Price, Benefits)\nQuantitative Position\", shape=ellipse];\n    Destination [label=\"Output: Winning/Losing Zone\nCategorical Outcome\", shape=ellipse];\n\n    Compass -> Coordinates [label=\"Interprets\"];\n    Coordinates -> Destination [label=\"Determines\"];\n\n    subgraph cluster_zones {\n        label=\"Zones\";\n        color=lightgray;\n        Winning [label=\"Winning Zone\n(e.g., More Benefits / Same Price)\", fillcolor=\"#90EE90\"];\n        Losing [label=\"Losing Zone\n(e.g., Less Benefits / More Price)\", fillcolor=\"#FFB6C1\"];\n        Destination -> Winning;\n        Destination -> Losing;\n    }\n}",
